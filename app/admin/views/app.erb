<% form_for :app, "/admin/app/save/#{params[:id]}", :id => 'admin-form', :multipart => true do |f| %>



<div class="dashhead">
  <div class="dashhead-titles">
    <h6 class="dashhead-subtitle">Dashboards</h6>
    <h2 class="dashhead-title">Apps</h2>
  </div>
  <div class="btn-toolbar dashhead-toolbar">
    <button class="btn btn-primary-outline">Save App</button>
  </div>
</div>

<div class="bd-example" data-example-id="">

<section class="content-header">
  <div class="field row" id="field_email">
    <div class="col-md-12">
      <div class="form-group">
        <%= f.label :name, :caption => "Name", :required => true %>
        <%= f.text_field :name, :class => 'form-control', :required => true %>
      </div><!-- /.form-group -->
    </div><!-- /.col -->
  </div><!-- /.row -->

  <div class="field row" id="field_content">
    <div class="col-md-12">
      <div class="form-group">
        <%= f.label :git_repo, :caption => "Git Repo", :required => true %>
        <%= f.text_field :git_repo, :class => 'form-control', :required => true %>
      </div><!-- /.form-group -->
    </div><!-- /.col -->
  </div><!-- /.row -->




  <!-- /.box -->
</section>
<% end %>





</div>